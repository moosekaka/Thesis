	month = oct,
	year = {2012},
	pages = {1005--1012},
	file = {Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\EBZ7WEM5\\Lam et al. - 2012 - Improving FRET dynamic range with bright green and.pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\U4BG7ZF6\\nmeth.2171.html:text/html}
