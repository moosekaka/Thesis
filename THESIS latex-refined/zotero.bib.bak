}

@article{sesaki_division_1999,
	title = {Division versus fusion: {Dnm}1p and {Fzo}1p antagonistically regulate mitochondrial shape},
	volume = {147},
	issn = {0021-9525},
	shorttitle = {Division versus fusion},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/10562274},
	abstract = {In yeast, mitochondrial division and fusion are highly regulated during growth, mating and sporulation, yet the mechanisms controlling these activities are unknown. Using a novel screen, we isolated mutants in which mitochondria lose their normal structure, and instead form a large network of interconnected tubules. These mutants, which appear defective in mitochondrial division, all carried mutations in DNM1, a dynamin-related protein that localizes to mitochondria. We also isolated mutants containing numerous mitochondrial fragments. These mutants were defective in FZO1, a gene previously shown to be required for mitochondrial fusion. Surprisingly, we found that in dnm1 fzo1 double mutants, normal mitochondrial shape is restored. Induction of Dnm1p expression in dnm1 fzo1 cells caused rapid fragmentation of mitochondria. We propose that dnm1 mutants are defective in the mitochondrial division, an activity antagonistic to fusion. Our results thus suggest that mitochondrial shape is normally controlled by a balance between division and fusion which requires Dnm1p and Fzo1p, respectively.},
	number = {4},
	urldate = {2012-06-30},
	journal = {J. Cell Biol.},
	author = {Sesaki, H and Jensen, R E},
	month = nov,
	year = {1999},
	pmid = {10562274},
	keywords = {Dynamin I, Dynamins, GTPase, Hexokinase, Membrane Proteins, Mitochondria, mitochondrial division, Mitochondrial dynamics, mitochondrial fusion, Structure-Function},
	pages = {699--706},
	file = {Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\97HN84W2\\Sesaki and Jensen - 1999 - Division versus Fusion Dnm1p and Fzo1p Antagonist.pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\874QB5XV\\699.html:text/html}
}

@article{zick_cristae_2009,
	series = {Assembly of the {Mitochondrial} {Respiratory} {Chain}},
	title = {Cristae formation—linking ultrastructure and function of mitochondria},
	volume = {1793},
	issn = {0167-4889},
	url = {http://www.sciencedirect.com/science/article/pii/S0167488908002401},
	doi = {10.1016/j.bbamcr.2008.06.013},
	abstract = {Mitochondria are double-membrane enclosed eukaryotic organelles with a central role in numerous cellular functions. The ultrastructure of mitochondria varies considerably between tissues, organisms, and the physiological state of cells. Alterations and remodeling of inner membrane structures are evident in numerous human disorders and during apoptosis. The inner membrane is composed of two subcompartments, the cristae membrane and the inner boundary membrane. Recent advances in electron tomography led to the current view that these membrane domains are connected by rather small tubular structures, termed crista junctions. They have been proposed to regulate the dynamic distribution of proteins and lipids as well as of soluble metabolites between individual mitochondrial subcompartments. One example is the release of cytochrome c upon induction of apoptosis. However, only little is known on the molecular mechanisms mediating the formation and maintenance of cristae and crista junctions. Here we review the current knowledge of the factors that determine cristae morphology and how the latter is linked to mitochondrial function. Further, we formulate several theoretical models which could account for the de novo formation of cristae as well as their propagation from existing cristae.},
	number = {1},
	urldate = {2015-06-09},
	journal = {Biochimica et Biophysica Acta (BBA) - Molecular Cell Research},
	author = {Zick, Michael and Rabl, Regina and Reichert, Andreas S.},
	month = jan,
	year = {2009},
	keywords = {Apoptosis, cristae, Cristae – matrix visualization, Electron tomography, Mitochondria, Mitochondrial dynamics, Structure-Function},
	pages = {5--19},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\TPKTSBVQ\\Zick et al. - 2009 - Cristae formation—linking ultrastructure and funct.pdf:application/pdf}
}

@article{oreilly_quantitative_2003,
	title = {Quantitative {Analysis} of {Spontaneous} {Mitochondrial} {Depolarizations}},
	volume = {85},
	issn = {0006-3495},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1303612/},
	abstract = {Spontaneous transient depolarizations in mitochondrial membrane potential (ΔΨm), mitochondrial flickers, have been observed in isolated mitochondria and intact cells using the fluorescent probe, tetramethylrhodamine ethyl ester (TMRE). In theory, the ratio of [TMRE] in cytosol and mitochondrion allows ΔΨm to be calculated with the Nernst equation, but this has proven difficult in practice due to fluorescence quenching and binding of dye to mitochondrial membranes. We developed a new method to determine the amplitude of flickers in terms of millivolts of depolarization. TMRE fluorescence was monitored using high-speed, high-sensitivity three-dimensional imaging to track individual mitochondria in freshly dissociated smooth muscle cells. Resting mitochondrial fluorescence, an exponential function of resting ΔΨm, varied among mitochondria and was approximately normally distributed. Spontaneous changes in mitochondrial fluorescence, indicating depolarizations and repolarizations in ΔΨm, were observed. The depolarizations were reversible and did not result in permanent depolarization of the mitochondria. The magnitude of the flickers ranged from {\textless}10 mV to {\textgreater}100 mV with a mean of 17.6 ± 1.0 mV (n = 360) and a distribution skewed to smaller values. Nearly all mitochondria flickered, and they did so independently of one another, indicating that mitochondria function as independent units in the myocytes employed here.},
	number = {5},
	urldate = {2015-10-02},
	journal = {Biophys J},
	author = {O'Reilly, Catherine M. and Fogarty, Kevin E. and Drummond, Robert M. and Tuft, Richard A. and Walsh, John V.},
	month = nov,
	year = {2003},
	pmid = {14581236},
	pmcid = {PMC1303612},
	pages = {3350--3357},
	file = {PubMed Central Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\PAKFRB6U\\O'Reilly et al. - 2003 - Quantitative Analysis of Spontaneous Mitochondrial.pdf:application/pdf}
}

