	file = {Murphy and Brand - 1988 - Membrane-potential-dependent changes in the stoich.pdf:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\EXTX8CG8\\Murphy and Brand - 1988 - Membrane-potential-dependent changes in the stoich.pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\ABVIPVGA\\abstract.html:text/html}
