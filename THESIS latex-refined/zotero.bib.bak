@article{rapaport_fzo1p_1998,
	title = {Fzo1p {Is} a {Mitochondrial} {Outer} {Membrane} {Protein} {Essential} for the {Biogenesis} of {Functional} {Mitochondria} in {Saccharomyces} cerevisiae},
	volume = {273},
