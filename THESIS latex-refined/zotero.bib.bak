@article{erjavec_selective_2008,
	title = {Selective benefits of damage partitioning in unicellular systems and its effects on aging},
	volume = {105},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/105/48/18764},
	doi = {10.1073/pnas.0804550105},
	abstract = {Cytokinesis in unicellular organisms sometimes entails a division of labor between cells leading to lineage-specific aging. To investigate the potential benefits of asymmetrical cytokinesis, we created a mathematical model to simulate the robustness and fitness of dividing systems displaying different degrees of damage segregation and size asymmetries. The model suggests that systems dividing asymmetrically (size-wise) or displaying damage segregation can withstand higher degrees of damage before entering clonal senescence. When considering population fitness, a system producing different-sized progeny like budding yeast is predicted to benefit from damage retention only at high damage propagation rates. In contrast, the fitness of a system of equal-sized progeny is enhanced by damage segregation regardless of damage propagation rates, suggesting that damage partitioning may also provide an evolutionary advantage in systems dividing by binary fission. Indeed, by using Schizosaccharomyces pombe as a model, we experimentally demonstrate that damaged proteins are unevenly partitioned during cytokinesis and the damage-enriched sibling suffers from a prolonged generation time and accelerated aging. This damage retention in S. pombe is, like in Saccharomyces cerevisiae, Sir2p- and cytoskeleton-dependent, suggesting this to be an evolutionarily conserved mechanism. We suggest that sibling-specific aging may be a result of the strong selective advantage of damage segregation, which may be more common in nature than previously anticipated.},
	language = {en},
	number = {48},
	urldate = {2015-10-22},
	journal = {PNAS},
	author = {Erjavec, N. and Cvijovic, M. and Klipp, E. and Nyström, T.},
	month = dec,
	year = {2008},
	pmid = {19020097},
	keywords = {asymmetry, computational modeling, fitness, protein damage},
	pages = {18764--18769},
	file = {Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\5IT9AVKD\\Erjavec et al. - 2008 - Selective benefits of damage partitioning in unice.pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\2NH74IKU\\18764.html:text/html}
}

@article{cole_measuring_2011,
	title = {Measuring and interpreting point spread functions to determine confocal microscope resolution and ensure quality control},
	volume = {6},
	copyright = {© 2011 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {1754-2189},
	url = {http://www.nature.com/nprot/journal/v6/n12/full/nprot.2011.407.html},
	doi = {10.1038/nprot.2011.407},
	abstract = {This protocol outlines a procedure for collecting and analyzing point spread functions (PSFs). It describes how to prepare fluorescent microsphere samples, set up a confocal microscope to properly collect 3D confocal image data of the microspheres and perform PSF measurements. The analysis of the PSF is used to determine the resolution of the microscope and to identify any problems with the quality of the microscope's images. The PSF geometry is used as an indicator to identify problems with the objective lens, confocal laser scanning components and other relay optics. Identification of possible causes of PSF abnormalities and solutions to improve microscope performance are provided. The microsphere sample preparation requires 2–3 h plus an overnight drying period. The microscope setup requires 2 h (1 h for laser warm up), whereas collecting and analyzing the PSF images require an additional 2–3 h.},
	language = {en},
	number = {12},
	urldate = {2013-12-19},
	journal = {Nat. Protocols},
	author = {Cole, Richard W. and Jinadasa, Tushare and Brown, Claire M.},
	month = dec,
	year = {2011},
	keywords = {protocols, PSF},
	pages = {1929--1941},
	file = {Cole et al. - 2011 - Measuring and interpreting point spread functions .pdf:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\56NXTXDT\\Cole et al. - 2011 - Measuring and interpreting point spread functions .pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\ZNE4GCMS\\Cole et al. - 2011 - Measuring and interpreting point spread functions .html:text/html}
