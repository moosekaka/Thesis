	issn = {2050-084X},
	doi = {10.7554/eLife.07739},
	abstract = {The conserved MICOS complex functions as a primary determinant of mitochondrial inner membrane structure. We address the organization and functional roles of MICOS and identify two independent MICOS subcomplexes: Mic27/Mic10/Mic12, whose assembly is dependent on respiratory complexes and the mitochondrial lipid cardiolipin, and Mic60/Mic19, which assembles independent of these factors. Our data suggest that MICOS subcomplexes independently localize to cristae junctions and are connected via Mic19, which functions to regulate subcomplex distribution, and thus, potentially also cristae junction copy number. MICOS subunits have non-redundant functions as the absence of both MICOS subcomplexes results in more severe morphological and respiratory growth defects than deletion of single MICOS subunits or subcomplexes. Mitochondrial defects resulting from MICOS loss are caused by misdistribution of respiratory complexes in the inner membrane. Together, our data are consistent with a model where MICOS, mitochondrial lipids and respiratory complexes coordinately build a functional and correctly shaped mitochondrial inner membrane.},
	language = {eng},
	journal = {Elife},
	author = {Friedman, Jonathan R. and Mourier, Arnaud and Yamada, Justin and McCaffery, J. Michael and Nunnari, Jodi},
	year = {2015},
	pmid = {25918844},
	pmcid = {PMC4434539}
}

@article{madden_cell_1998,
	title = {Cell polarity and morphogenesis in budding yeast},
	volume = {52},
	issn = {0066-4227},
	doi = {10.1146/annurev.micro.52.1.687},
	abstract = {Eukaryotic cells respond to intracellular and extracellular cues to direct asymmetric cell growth and division. The yeast Saccharomyces cerevisiae undergoes polarized growth at several times during budding and mating and is a useful model organism for studying asymmetric growth and division. In recent years, many regulatory and cytoskeletal components important for directing and executing growth have been identified, and molecular mechanisms have been elucidated in yeast. Key signaling pathways that regulate polarization during the cell cycle and mating response have been described. Since many of the components important for polarized cell growth are conserved in other organisms, the basic mechanisms mediating polarized cell growth are likely to be universal among eukaryotes.},
	language = {eng},
	journal = {Annu. Rev. Microbiol.},
	author = {Madden, K. and Snyder, M.},
	year = {1998},
	pmid = {9891811},
	keywords = {Calcium-Calmodulin-Dependent Protein Kinases, Cell Cycle, Cell Division, Cell Polarity, Cyclin-Dependent Kinases, Cyclin G, Cyclins, Cytoskeleton, Fungal Proteins, Genes, Fungal, Pheromones, Saccharomyces cerevisiae, Saccharomycetales},
	pages = {687--744}
}

@article{bellou_morphological_2014,
	title = {Morphological and metabolic shifts of {Yarrowia} lipolytica induced by the alteration of the dissolved oxygen concentration in the growth environment},
	issn = {1350-0872, 1465-2080},
	url = {http://mic.sgmjournals.org/content/early/2014/02/06/mic.0.074302-0},
	doi = {10.1099/mic.0.074302-0},
	abstract = {Yarrowia lipolytica, an ascomycete with biotechnological potential, is able to form either yeast cells or hyphae and pseudohyphae in response to environmental conditions. We report in this paper that the morphology of Y. lipolytica, cultivated in batch cultures on hydrophilic (glucose and glycerol) and hydrophobic (olive oil) media, was not affected by the nature of the carbon source, neither the nature nor the concentration of the nitrogen source. On the contrary, dissolved oxygen concentration (DOC) should be considered as the major factor affecting yeast morphology. Specifically, when growth occurred in low or zero DOC the mycelial and/or pseudomycelial forms predominated over the yeast form independently of the carbon and nitrogen sources used. Experimental data originated from a continuous culture of Y. lipolytica on glycerol, being used as carbon and energy source, demonstrate that transition of mycelium to yeast form occurs when DOC increases from 0.1 mg l-1 to 1.5 mg l-1. DOC affected also the yeast physiology since the activity of enzymes implicated in lipid biosynthesis process (i.e. ATP citrate lyase, malic enzyme) was up-regulated at high DOC whereas the activity of enzymes implicated in glycerol assimilation (such as glycerol dehydrogenase and kinase) remained fundamentally unaffected in the cell free extract.},
	language = {en},
	urldate = {2014-04-11},
	journal = {Microbiology},
	author = {Bellou, Stamatia and Makri, Anna and Triantaphyllidou, Irene-Eva and Papanikolaou, Seraphim and Aggelis, George},
	month = feb,
	year = {2014},
	pmid = {24509502},
	keywords = {Batch and continuous cultivation modes, Bioreactor, Dissolved oxygen concentration, Hydrophilic and hydrophobic substrates, Yarrowia lipolytica},
	pages = {mic.0.074302--0},
	file = {Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\284CWQPG\\Bellou et al. - 2014 - Morphological and metabolic shifts of Yarrowia lip.pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\AZ7M8NZX\\mic.0.074302-0.html:text/html}
}

@article{starovoytova_mitochondrial_2013,
	title = {Mitochondrial signaling in {Saccharomyces} cerevisiae pseudohyphae formation induced by butanol},
	copyright = {© 2013 Federation of European Microbiological Societies. Published by Blackwell Publishing Ltd. All rights reserved},
	issn = {1567-1364},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/1567-1364.12039/abstract},
	doi = {10.1111/1567-1364.12039},
	abstract = {Yeasts growing limited for nitrogen source or treated with fusel alcohols form elongated cells – pseudohyphae. Absence of mitochondrial DNA or anaerobic conditions inhibits this process, but the precise role of mitochondria is not clear. We found that a significant percentage of pseudohyphal cells contained mitochondria with different levels of membrane potential within one cell. An uncoupler FCCP, but not the ATP-synthase inhibitor oligomycin D, prevented pseudohyphal growth. Interestingly, repression of the MIH1 gene encoding phosphatase activator of the G2/M transition partially restores the ability of yeast to form pseudohyphal cells in the presence of FCCP or in the absence of mitochondrial DNA. At the same time, retrograde signaling (the one triggered by dysfunctional mitochondria) appeared to be a positive regulator of butanol-induced pseudohyphae formation: the deletion of any of the retrograde signaling genes (RTG1, RTG2, or RTG3) partially suppressed pseudohyphal growth. Together, our data suggests that two subpopulations of mitochondria are required for filamentous growth: one with high and another with low transmembrane potential. These mitochondria activated signaling pathways appear to converge at Mih1p level.© 2013 Federation of European Microbiological Societies. Published by Blackwell Publishing Ltd. All rights reserved},
	language = {en},
	urldate = {2013-03-04},
	journal = {FEMS Yeast Research},
	author = {Starovoytova, Anna N. and Sorokin, Maxim I. and Sokolov, Svyatoslav S. and Severin, Fedor F. and Knorre, Dmitry A.},
	year = {2013},
	keywords = {Cell Cycle, filamentous growth, Hydrogen Peroxide},
	pages = {n/a--n/a},
	file = {Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\QT8J3UPX\\Starovoytova et al. - 2013 - Mitochondrial signaling in Saccharomyces cerevisia.pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\RJ3VAS7R\\abstract.html:text/html}
}

@article{kim_automated_2010,
	title = {Automated tracking of yeast cell lineages},
	volume = {7798},
	issn = {0277786X},
	url = {http://spiedigitallibrary.org/proceedings/resource/2/psisdg/7798/1/779823_1},
	doi = {doi:10.1117/12.859770},
	abstract = {We propose a cell progeny tracking method that sequentially employs image alignment, chamber cropping, cell segmentation, per-cell feature measurement, and progeny (lineage) tracking modules. It enables biologists to keep track of phenotypic patterns not only over time but also over multiple generations. Yeast cells encapsulated in chambers of a polydimethylsiloxane (PDMS) microfluidic device were imaged over time to monitor changes in fluorescence levels. We implemented our method in an automated cell image analysis tool, CellProfiler, and performed initial testing. Once refined and validated, the approach could be adapted/used in other cell segmentation and progeny tracking experiments.},
	number = {1},
	urldate = {2012-07-27},
