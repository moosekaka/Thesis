}

@article{akemann_imaging_2010,
	title = {Imaging brain electric signals with genetically targeted voltage-sensitive fluorescent proteins},
	volume = {7},
	copyright = {© 2010 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
