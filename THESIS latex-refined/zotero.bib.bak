@article{murphy_membrane-potential-dependent_1988,
	title = {Membrane-potential-dependent changes in the stoichiometry of charge translocation by the mitochondrial electron transport chain},
	volume = {173},
	issn = {1432-1033},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1432-1033.1988.tb14046.x/abstract},
	doi = {10.1111/j.1432-1033.1988.tb14046.x},
	abstract = {The charge/oxygen (q+/O) stoichiometry of mitochondria respiring on succinate was measured under conditions of high membrane potential (ΔΨ). The technique used was a variation of the steady-state method of Al-Shawi and Brand [(1981) Biochem. J. 200, 539–546]. We show that q+/O was about 2.7 at high values of ΔΨ (170 mV). As ΔΨ was lowered from 170 mV to 85 mV with the respiratory inhibitor malonate the q+/O stoichiometry increased to 6.0. A number of artefacts which could have led to an underestimation of the q+/O stoichiometry were eliminated. These included effects of any rapid change in mitochondrial volume, internal pH, activity of the endogenous K+/H+ exchanger or in H+ conductance due to changes in ΔΨ after the addition of inhibitor. The experiments presented here are the first direct demonstration that the stoichiometry of proton pumping by the mitochondrial respiratory chain changes as ΔΨ is varied.},
