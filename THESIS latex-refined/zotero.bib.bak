}

@article{saa_modelling_2013,
	title = {Modelling the {ATP} production in mitochondria},
	volume = {75},
	issn = {0092-8240, 1522-9602},
	url = {http://arxiv.org/abs/1212.1194},
	doi = {10.1007/s11538-013-9862-1},
	abstract = {We revisit here the mathematical model for ATP production in mitochondria introduced recently by Bertram, Pedersen, Luciani, and Sherman (BPLS) as a simplification of the more complete but intricate Magnus and Keizer's model. We identify some inaccuracies in the BPLS original approximations for two flux rates, namely the adenine nucleotide translocator rate \$J\_\{{\textbackslash}rm ANT\}\$ and the calcium uniporter rate \$J\_\{{\textbackslash}rm uni\}\$. We introduce new approximations for such flux rates and then analyze some of the dynamical properties of the model. We infer, from exhaustive numerical explorations, that the enhanced BPLS equations have a unique attractor fixed point for physiologically acceptable ranges of mitochondrial variables and respiration inputs, as one would indeed expect from homeostasis. We determine, in the stationary regime, the dependence of the mitochondrial variables on the respiration inputs, namely the cytosolic concentration of calcium \$\{{\textbackslash}rm Ca\}\_\{{\textbackslash}rm c\}\$ and the substrate fructose 1,6-bisphosphate FBP. The same dynamical effects of calcium and FBP saturations reported for the original BPLS model are observed here. We find out, however, a novel non-stationary effect which could be, in principle, physiologically interesting: some response times of the model tend to increase considerably for high concentrations of calcium and/or FBP. In particular, the larger the concentrations of \$\{{\textbackslash}rm Ca\}\_\{{\textbackslash}rm c\}\$ and/or FBP, the larger the necessary time to attain homeostasis.},
	number = {9},
	urldate = {2015-09-15},
	journal = {Bulletin of Mathematical Biology},
	author = {Saa, Alberto and Siqueira, Kellen M.},
	month = sep,
	year = {2013},
	note = {arXiv: 1212.1194},
	keywords = {Physics - Biological Physics, Quantitative Biology - Molecular Networks},
	pages = {1636--1651},
	file = {arXiv\:1212.1194 PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\CS9GRIMS\\Saa and Siqueira - 2013 - Modelling the ATP production in mitochondria.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\QHXWCTFR\\1212.html:text/html;Modeling the ATP Production in Mitochondria - Spri.pdf:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\G9PF3RMW\\Modeling the ATP Production in Mitochondria - Spri.pdf:application/pdf}
}

