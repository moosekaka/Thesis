	issn = {1567-7249},
	url = {http://www.sciencedirect.com/science/article/pii/S156772490800038X},
	doi = {10.1016/j.mito.2008.06.001},
	abstract = {Cells are exposed during their life span to fluctuating levels of reactive oxygen species (ROS). To investigate the effects of a single ROS boost in vitro, human endothelial cells (HUVEC) were treated with one short-term dose of hydrogen peroxide. This treatment resulted in a short, dose-dependent ROS peak that caused transient changes in the mitochondrial morphology and fine structure, in the frequency of mitochondrial fission and fusion and in the mRNA levels of distinct fission and fusion factors. Treatment with a higher dose induced prolonged mtDNA damage; these cells exhibited a significantly shortened replicative lifespan, indicating dose-dependent effects of oxidative stress on mitochondria.},
	number = {4},
	urldate = {2015-10-31},
	journal = {Mitochondrion},
	author = {Jendrach, Marina and Mai, Sören and Pohl, Sandra and Vöth, Monika and Bereiter-Hahn, Jürgen},
	month = sep,
	year = {2008},
	keywords = {Ageing, Fission, Fusion, Hydrogen Peroxide, Mitochondria, mtDNA, ROS},
	pages = {293--304},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\XWPDUF8A\\Jendrach et al. - 2008 - Short- and long-term alterations of mitochondrial .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\AKIGIBNQ\\S156772490800038X.html:text/html}
