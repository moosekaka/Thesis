@article{bliek_mechanisms_2013,
	title = {Mechanisms of {Mitochondrial} {Fission} and {Fusion}},
	volume = {5},
	issn = {, 1943-0264},
	url = {http://cshperspectives.cshlp.org/content/5/6/a011072},
	doi = {10.1101/cshperspect.a011072},
	abstract = {Mitochondria continually change shape through the combined actions of fission, fusion, and movement along cytoskeletal tracks. The lengths of mitochondria and the degree to which they form closed networks are determined by the balance between fission and fusion rates. These rates are influenced by metabolic and pathogenic conditions inside mitochondria and by their cellular environment. Fission and fusion are important for growth, for mitochondrial redistribution, and for maintenance of a healthy mitochondrial network. In addition, mitochondrial fission and fusion play prominent roles in disease-related processes such as apoptosis and mitophagy. Three members of the Dynamin family are key components of the fission and fusion machineries. Their functions are controlled by different sets of adaptor proteins on the surface of mitochondria and by a range of regulatory processes. Here, we review what is known about these proteins and the processes that regulate their actions.},
	language = {en},
	number = {6},
	urldate = {2015-09-12},
	journal = {Cold Spring Harb Perspect Biol},
	author = {Bliek, Alexander M. van der and Shen, Qinfang and Kawajiri, Sumihiro},
	month = jun,
	year = {2013},
	pmid = {23732471},
	keywords = {Fission, fusion, Mitochondrial dynamics},
	pages = {a011072},
	file = {Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\RPHU5H9D\\Bliek et al. - 2013 - Mechanisms of Mitochondrial Fission and Fusion.pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\T2DZBQJ7\\a011072.html:text/html}
}

@article{frezza_opa1_2006,
	title = {{OPA}1 {Controls} {Apoptotic} {Cristae} {Remodeling} {Independently} from {Mitochondrial} {Fusion}},
	volume = {126},
	issn = {0092-8674},
	url = {http://www.sciencedirect.com/science/article/pii/S0092867406008105},
	doi = {10.1016/j.cell.2006.06.025},
	abstract = {Summary
Mitochondria amplify activation of caspases during apoptosis by releasing cytochrome c and other cofactors. This is accompanied by fragmentation of the organelle and remodeling of the cristae. Here we provide evidence that Optic Atrophy 1 (OPA1), a profusion dynamin-related protein of the inner mitochondrial membrane mutated in dominant optic atrophy, protects from apoptosis by preventing cytochrome c release independently from mitochondrial fusion. OPA1 does not interfere with activation of the mitochondrial “gatekeepers” BAX and BAK, but it controls the shape of mitochondrial cristae, keeping their junctions tight during apoptosis. Tightness of cristae junctions correlates with oligomerization of two forms of OPA1, a soluble, intermembrane space and an integral inner membrane one. The proapoptotic BCL-2 family member BID, which widens cristae junctions, also disrupts OPA1 oligomers. Thus, OPA1 has genetically and molecularly distinct functions in mitochondrial fusion and in cristae remodeling during apoptosis.},
	number = {1},
	urldate = {2015-10-28},
	journal = {Cell},
	author = {Frezza, Christian and Cipolat, Sara and Martins de Brito, Olga and Micaroni, Massimo and Beznoussenko, Galina V. and Rudka, Tomasz and Bartoli, Davide and Polishuck, Roman S. and Danial, Nika N. and De Strooper, Bart and Scorrano, Luca},
	month = jul,
	year = {2006},
	pages = {177--189},
	file = {ScienceDirect Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\4U7PMAMH\\S0092867406008105.html:text/html}
}

@phdthesis{compton_high-throughput_2014,
	address = {United States -- California},
	type = {Ph.{D}.},
	title = {High-throughput microcavitation bubble induced cellular mechanotransduction},
	copyright = {Copyright ProQuest, UMI Dissertations Publishing 2014},
	url = {http://search.proquest.com/docview/1524266213/abstract/AB6F5AEA26AC4B9EPQ/4},
	abstract = {Focused pulsed laser irradiation allows for the deposition of energy with high spatial and temporal resolution. These attributes provide an optimal tool for non-contact manipulation in cellular biology such as laser microsurgery, cell membrane permeabilization, as well as targeted cell death.
In this thesis we investigate the direct physical effects produced by laser- generated microcavitation bubbles in adherent cell cultures. We examine how variation in pulse durations (180 ps - 6ns) and pulse energy (0.5 - 40 μ;J) affect microcavitation bubble ( μ ;CB) generated cell lysis, necrosis, and molecular delivery. To compare the effects of pulse duration we employ classical fluid dynamics modeling to quantify the perturbation caused on cell populations from μ ;CB generated microTsunamis (a transient microscale burst of hydrodynamic shear stress). Through time-resolved imaging we capture the μ ;CB dynamics at various energies and pulse durations. Moreover, the mathematical modeling provides information regarding the cellular exposure to time varying shear stress and impulse as a function of radial location from the μ ;CB center. We demonstrate that the resultant cellular effect can be predicted based on the total impulse across a two order of magnitude span of pulse duration and pulse energy.
We also examine the region of cells beyond the zone of molecular delivery to investigate possible cellular reactions to μ ;Tsunami exposure. Our studies have shown that cellular mechanotransduction occurs within cell populations spanning an area of up to 1 mm2 surrounding the μ ;CB. Visualization of mechanotransduction is achieved through the visualization of intracellular calcium signaling via fluorescence microscopy that occurs due to the ability of the μ Tsunami generated shear stresses to stimulate G-protein coupled receptors at the apical cell surface. Moreover, we have shown that the observed signaling can be attenuated in a dose-dependent manner using 2-APB which is a known inhibitor to IP 3 induced Ca2+ release. This capability opens the development of a high-throughput screening platform for molecules that modulate cellular mechanotransduction. We have applied this approach to screen the effects of a small set of small molecules, in a 96-well plate in less than an hour. These detailed studies offer a basis for the design, development, and implementation of a novel high-throughput mechanotransduction assay to rapidly screen the effect of small molecules on cellular mechanotransduction at high throughput.},
	language = {English},
	urldate = {2015-09-24},
	school = {University of California, Irvine},
	author = {Compton, Jonathan Lee},
	year = {2014},
	keywords = {Applied sciences, Biological sciences, Cavitation bubble, Drug discovery, Fluorescence imaging, Gilmore model, Intracellular calcium, Mechanotransduction, Pure sciences},
