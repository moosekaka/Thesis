	volume = {2012},
	issn = {1687-8876},
	shorttitle = {The {Many} {Faces} of {Mitochondrial} {Autophagy}},
	url = {http://www.hindawi.com/journals/ijcb/2012/431684/abs/},
	doi = {10.1155/2012/431684},
	abstract = {Research into the selective autophagic degradation of mitochondria\&\#8212;mitophagy\&\#8212;has intensified in recent years, yielding significant insights into the function, mechanism, and regulation of this process in the eukaryotic cell. However, while some molecular players in budding yeast, such as Atg32p, Uth1p, and Aup1p, have been identified, studies further interrogating the mechanistic and regulatory features of mitophagy have yielded inconsistent and sometimes conflicting results. In this review, we focus on the current understanding of mitophagy mechanism, induction, and regulation in yeast, and suggest that differences in experimental conditions used in the various studies of mitophagy may contribute to the observed discrepancies. Consideration and understanding of these differences may help place the mechanism and regulation of mitophagy in context, and further indicate the intricate role that this essential process plays in the life and death of eukaryotic cells.},
	language = {en},
	urldate = {2014-06-09},
	journal = {International Journal of Cell Biology},
	author = {May, Alexander I. and Devenish, Rodney J. and Prescott, Mark},
	month = apr,
	year = {2012},
	keywords = {mitophagy},
	pages = {e431684},
	file = {Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\4VKH7XZ8\\May et al. - 2012 - The Many Faces of Mitochondrial Autophagy Making .pdf:application/pdf;Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\72BDKCV8\\431684.html:application/xhtml+xml}
}

