@article{figge_deceleration_2012,
	title = {Deceleration of {Fusion}–{Fission} {Cycles} {Improves} {Mitochondrial} {Quality} {Control} during {Aging}},
	volume = {8},
	url = {http://dx.doi.org/10.1371/journal.pcbi.1002576},
	doi = {10.1371/journal.pcbi.1002576},
	abstract = {Author SummaryMitochondria are organelles that play a central role as ‘cellular power plants’. The cellular organization of these organelles involves a dynamic spatial network where mitochondria constantly undergo fusion and fission associated with the mixing of their molecular content. Together with the processes of mitophagy and biogenesis of mitochondrial mass, this results into a cellular surveillance system for maintaining their bioenergetic quality. The accumulation of molecular damage in mitochondria is associated with various human disorders and with aging. However, how these processes affect aging and how they can be reconciled with existing aging theories is just at the beginning to be considered. Mathematical modeling allows simulating the dynamics of mitochondrial quality control during aging in silico and leads to the ‘mitochondrial infectious damage adaptation’ (MIDA) model of aging. It reconciles a number of counterintuitive observations obtained during the last decade including infection-like processes of molecular damage spread, the reduction of fusion and fission rates during cellular aging, and observed life span extension for reduced mitochondrial fission. Interestingly, the MIDA model suggests that a reduction in mitochondrial dynamics rather than being merely a sign or even cause of aging, may actually reflect a systemic adaptation to prolong organismic life span.},
	number = {6},
	urldate = {2015-06-07},
	journal = {PLoS Comput Biol},
	author = {Figge, Marc Thilo and Reichert, Andreas S. and Meyer-Hermann, Michael and Osiewacz, Heinz D.},
	month = jun,
	year = {2012},
	keywords = {Mitochondrial dynamics, Structure-Function},
	pages = {e1002576},
	file = {PLoS Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\V8B2IFGW\\Figge et al. - 2012 - Deceleration of Fusion–Fission Cycles Improves Mit.pdf:application/pdf}
}

@incollection{thorn_chapter_2010,
	series = {Guide to {Yeast} {Genetics}: {Functional} {Genomics}, {Proteomics}, and {Other} {Systems} {Analysis}},
	title = {Chapter 23 - {Spinning}-{Disk} {Confocal} {Microscopy} of {Yeast}},
	volume = {Volume 470},
	isbn = {0076-6879},
	url = {http://www.sciencedirect.com/science/article/pii/S0076687910700239},
	abstract = {Spinning-disk confocal microscopy is an imaging technique that combines the out-of-focus light rejection of confocal microscopy with the high sensitivity of wide-field microscopy. Because of its unique features, it is well suited to high-resolution imaging of yeast and other small cells. Elimination of out-of-focus light significantly improves the image contrast and signal-to-noise ratio, making it easier to resolve and quantitate small, dim structures in the cell. These features make spinning-disk confocal microscopy an excellent technique for studying protein localization and dynamics in yeast. In this review, I describe the rationale behind using spinning-disk confocal imaging for yeast, hardware considerations when assembling a spinning-disk confocal scope, and methods for strain preparation and imaging. In particular, I discuss choices of objective lens and camera, choice of fluorescent proteins for tagging yeast genes, and methods for sample preparation.},
	urldate = {2013-12-13},
	booktitle = {Methods in {Enzymology}},
	publisher = {Academic Press},
	author = {Thorn, Kurt},
	editor = {{Jonathan Weissman; Christine Guthrie and Gerald R. Fink}},
	year = {2010},
	pages = {581--602},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\X334526S\\Thorn and Jonathan Weissman\; Christine Guthrie and Gerald R. Fink - 2010 - Chapter 23 - Spinning-Disk Confocal Microscopy of .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\2U5NGFUA\\Thorn and Jonathan Weissman\; Christine Guthrie and Gerald R. Fink - 2010 - Chapter 23 - Spinning-Disk Confocal Microscopy of .html:text/html}
