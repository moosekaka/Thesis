@article{lee_improved_2013,
	title = {Improved {Blue}, {Green}, and {Red} {Fluorescent} {Protein} {Tagging} {Vectors} for {S}. cerevisiae},
	volume = {8},
	url = {http://dx.doi.org/10.1371/journal.pone.0067902},
	doi = {10.1371/journal.pone.0067902},
	abstract = {Fluorescent protein fusions are a powerful tool to monitor the localization and trafficking of proteins. Such studies are particularly easy to carry out in the budding yeast Saccharomyces cerevisiae due to the ease with which tags can be introduced into the genome by homologous recombination. However, the available yeast tagging plasmids have not kept pace with the development of new and improved fluorescent proteins. Here, we have constructed yeast optimized versions of 19 different fluorescent proteins and tested them for use as fusion tags in yeast. These include two blue, seven green, and seven red fluorescent proteins, which we have assessed for brightness, photostability and perturbation of tagged proteins. We find that EGFP remains the best performing green fluorescent protein, that TagRFP-T and mRuby2 outperform mCherry as red fluorescent proteins, and that mTagBFP2 can be used as a blue fluorescent protein tag. Together, the new tagging vectors we have constructed provide improved blue and red fluorescent proteins for yeast tagging and three color imaging.},
	number = {7},
	urldate = {2015-04-02},
	journal = {PLoS ONE},
	author = {Lee, Sidae and Lim, Wendell A. and Thorn, Kurt S.},
	month = jul,
	year = {2013},
	pages = {e67902},
	file = {PLoS Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\JNEQB5WK\\Lee et al. - 2013 - Improved Blue, Green, and Red Fluorescent Protein .pdf:application/pdf;References_S1.docx:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\JSVV7PSR\\References_S1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Sequences_S1.docx:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\8WWJI4NM\\Sequences_S1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Table_S1.docx:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\ZMRGXTK9\\Table_S1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Table_S2.docx:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\MQZERPD2\\Table_S2.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Table_S3.docx:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\65BFKF4V\\Table_S3.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Table_S4.docx:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\M8ZCTH2J\\Table_S4.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Table_S5.docx:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\NDA8PFCC\\Table_S5.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document}
