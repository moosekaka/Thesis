}

@article{ugidos_perspectives_2010,
	title = {Perspectives on the mitochondrial etiology of replicative aging in yeast},
	volume = {45},
	issn = {0531-5565},
	shorttitle = {Special {Issue}: {Mitochondria} in aging and age-related disease},
	url = {http://www.sciencedirect.com/science/article/pii/S0531556510000550},
	doi = {10.1016/j.exger.2010.02.002},
	abstract = {In a now classical paper, Denham Harman suggested that free radicals produced during mitochondrial respiration cause cumulative oxidative damage, resulting in aging and age-related disorders and pathologies. Proponents of this hypothesis have focused their attention, not surprisingly, on mitochondria arguing that these organelles may serve as the biological clock for aging. Indeed, work on many models, including filamentous fungi, nematodes, and mammals have revealed that age-dependent reorganizations of the mitochondrial DNA (mtDNA) may play a central role in the aging of these organisms. Furthermore, genetic alterations of mitochondrial function may either shorten or extend life span. In this paper, we focus on the role of mitochondria in the replicative aging of yeast mother cells, whether this role of mitochondria is really a linked to altered ROS production and/or respiration, and highlight some important questions that remain to be answered.},
	number = {7–8},
	urldate = {2012-10-10},
	journal = {Experimental Gerontology},
	author = {Ugidos, Ana and Nyström, Thomas and Caballero, Antonio},
	month = aug,
	year = {2010},
	keywords = {aging, Caloric restriction, Mitochondria, Replicative life span, Yeast},
	pages = {512--515},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\FW7IAPE4\\Ugidos et al. - 2010 - Perspectives on the mitochondrial etiology of repl.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\swee\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\jj23uftl.default\\zotero\\storage\\4887NEV7\\S0531556510000550.html:text/html}
}

